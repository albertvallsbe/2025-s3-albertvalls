    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "./src/js",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "experimentalDecorators": true
    },
    "include": ["src/**/*.ts"]
}
